<!DOCTYPE html>
<html>
<head>
  <title>Dsi Map</title>
  <%= yield :head %>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <link rel="icon" href="<%= image_path("favicon.ico") %>" type="image/x-icon">
  <link rel="shortcut icon" href="<%= image_path("favicon.ico") %>" type="image/x-icon">
  <%= stylesheet_link_tag    "extjs4/resources/css/ext-all", :media => "all" %>
  <%= stylesheet_link_tag    "extjs4/examples/shared/example", :media => "all" %>
  <%= stylesheet_link_tag    "geoext2/resources/css/tree", :media => "all" %>
  <%= stylesheet_link_tag    "dsi", :media => "all" %>
  <link href="http://www.google.com/help/maps/streetview/css/streetview-style.css" rel="stylesheet">
  <%= javascript_include_tag "application" %>
  <%= javascript_include_tag "extjs4/ext-all" %>  
  <script type="text/javascript" src="https://www.google.com/jsapi"> </script>
  <script src="http://maps.google.com/maps/api/js?v=3&amp;sensor=false"></script>
  <%= javascript_include_tag "OpenLayers-2.12/OpenLayers-dsi" %>
  <%= javascript_include_tag "OpenLayers-2.12/lib/OpenLayers/Control/DeleteFeature" %>
  <%= javascript_include_tag "map_utils" %>
  <%= javascript_include_tag "jquery.printElement.min" %>
  <%= javascript_include_tag "dsi" %>
  <%= javascript_include_tag "info.json?var=printCapabilities" %>
  <%= csrf_meta_tags %>
</head>
<body>
<div id='google_street'></div>
<%= yield %>

</body>
</html>
